/* components/mall/order/orderList/orderList.wxss */
.mallorder_picbox {
  height: 144rpx;
  background: #f5f5f6;
  border-radius: 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  margin: 24rpx 24rpx 0 24rpx;
  padding: 0 24rpx;
  position: relative;
}
.more{
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}

.orderpic+.orderpic{
  margin-left: 16rpx;
}
.orderpic {
  height: 96rpx;
  width: 96rpx;
  box-sizing: content-box;
  margin-top: 24rpx;
}